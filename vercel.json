{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node",
      "config": { "distDir": "dist" }
    },
    {
      "src": "server/typedefs/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "server/resolvers/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "index.ts"
    }
  ]
}
